@using ENRLReconSystem.Utility
@using ENRLReconSystem.DO
@model List<DOGEN_BulkImport>


<div class="table-responsive">
@if (Model.Count > 0)
{
    <table class="table table-bordered" id="tblBulkUploadResult">
        <thead>
            <tr>
                <th class="sorting_disabled">Bulk Import ID</th>
                <th>Excel Template Name</th>
                <th>Import Status</th>
                <th>Total Records</th>
                <th>Valid Records</th>
                <th>Invalid Records</th>
                <th>Duplicate Records</th>
                <th>Created By</th>
                <th>Created On</th>
                <th>Updated By</th>
                <th>Updated On</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
             <tr>
                <td>@item.GEN_BulkImportId</td>
                <td>@item.ExcelFileName</td>
                <td>@item.ImportStatus</td>
                <td>@item.TotalRecordsCount</td>
                <td>@item.ValidRecordsCount</td>
                <td>@item.InvalidRecordsCount</td>
                <td>@item.DuplicateRecordCount</td>
                <td>@item.CreatedBy</td>
                <td>@item.UTCCreatedOn.ToDateTime().ToString("MM/dd/yyyy hh:mm tt")</td>
                <td>@item.LastUpdatedBy</td>
                <td>@item.UTCLastUpdatedOn.ToDateTime().ToString("MM/dd/yyyy hh:mm tt")</td>
             </tr>
            }
        </tbody>
    </table>
}
else
{
    <div style="text-align:center;color:red;">
        No Records Found
    </div>
}
</div>
