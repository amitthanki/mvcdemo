        // Date Validations.
        function isDatesValid() {
            var FirstLetterMailDate = $('#FirstLetterMailDate_DPart').val() != "" ? new Date($('#FirstLetterMailDate_DPart').val()) : $('#FirstLetterMailDate_DPart').val();
            var SecondLetterMailDate = $('#SecondLetterMailDate_DPart').val() != "" ? new Date($('#SecondLetterMailDate_DPart').val()) : $('#SecondLetterMailDate_DPart').val();
            var InitialAddressDate = $('#InitialAddressVerificationDate_DPart').val() != "" ? new Date($('#InitialAddressVerificationDate_DPart').val()) : $('#InitialAddressVerificationDate_DPart').val();
            var MemberResponseDate = $('#MemberResponseVerificationDate_DPart').val() != "" ? new Date($('#MemberResponseVerificationDate_DPart').val()) : $('#MemberResponseVerificationDate_DPart').val();

            var today = new Date();
            var errorList = "";
            var isValid = true;
            today.setHours(0, 0, 0, 0)

            //Date Of Birth Validation
            if (DOB != "") {
                if (DOB > today) {
                    $('#DateofBirth_DPart').removeClass("valid").addClass("input-validation-error");
                    errorList = errorList + '<li>Date Of Birth Should be Past Date</li>';
                    isValid = false;
                }
            }

            //Effective Date validation
            if (EffectiveDate != "") {
                if (EffectiveDate < DOB) {
                    $('#EffectiveDate_DPart').removeClass("valid").addClass("input-validation-error");
                    errorList = errorList + '<li>Effective Date should be more than Date Of Birth</li>';
                    isValid = false;
                }
            }

            if (isValid) {
                $('#dvErrorMsg').hide();
            } else {
                $('#dateError').html(errorList);
                $('#dvErrorMsg').show();
            }
            return isValid;
