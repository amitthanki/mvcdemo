                                                </div>
                                                <div class="form-group">
                                                    <label class="col-md-6">First Name</label>
                                                    @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.FirstName, new { @tabindex = "-1",@readonly = "readonly", @class = "form-control" })
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-md-6">Address Line 1</label>
                                                    @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.AddressLine1, new { @tabindex = "-1",@readonly = "readonly", @class = "form-control" })
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-md-6">Zip Code 5</label>
                                                    @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.ZipCode5, new { @tabindex = "-1",@readonly = "readonly", @class = "form-control" })
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-md-6">State</label>
                                                    @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.State, new { @tabindex = "-1",@readonly = "readonly", @class = "form-control" })
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-md-6">Plan Term Date</label>
                                                    @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.PlanTermDate, "{0:MM/dd/yyyy}", new { @tabindex = "-1",@readonly = "readonly", @class = "form-control" })
                                                </div>
