                                                    <label class="col-md-6">SCC Effective Date</label>
                                                    @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.SCCEffectiveDate, "{0:MM/dd/yyyy}", new { @tabindex = "-1",@readonly = "readonly", @class = "form-control" })
                                                </div>
                                                <div class="form-group">
