                                                </div>
                                                <div class="form-group">
                                                    <label class="col-md-6">City</label>
                                                    @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.City, new { @tabindex = "-1",@readonly = "readonly", @class = "form-control" })
                                                </div>

                                                <div class="form-group">
                                                    <label class="col-md-6">Plan Effective Date</label>
                                                    @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.PlanEffectiveDate, "{0:MM/dd/yyyy}", new { @tabindex = "-1",@readonly = "readonly", @class = "form-control" })
                                                </div>


