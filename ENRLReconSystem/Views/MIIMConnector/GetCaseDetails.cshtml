                                                    @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.OOAIndicator, new { @tabindex = "-1",@readonly = "readonly", @class = "form-control" })
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-md-6">Application Approved Status</label>
                                                    @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.ApplicationApprovedStatus, new { @tabindex = "-1",@readonly = "readonly", @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="tab-pane-section col-md-4">
                                                <div class="form-group">
                                                    <label class="col-md-6">Member ID</label>
                                                    @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.MemberId, new { @tabindex = "-1",@readonly = "readonly", @class = "form-control" })
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-md-6">Line of Business Desc</label>
