                <label class="col-md-6">Date of Birth</label>
                @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.DOB, "{0:MM/dd/yyyy}", new { @tabindex = "-1", @readonly = "readonly", @class = "form-control" })
            </div>
            <div class="form-group">
                <label class="col-md-6">First Name</label>

                <div class="tool-tip-box clearfix">
                    @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.FirstName, new { @tabindex = "-1", @readonly = "readonly", @Value = TruncateAndPaddString(Model.objDOGEN_GPSData.FirstName), @class = "form-control" })
