@using ENRLReconSystem.Utility
@using ENRLReconSystem.Helpers
@using ENRLReconSystem.DO
@model DOGEN_Queue

@functions {
    private string TruncateAndPaddString(string text)
    {
        return text = (text.NullToString().Length > 20) ? text.Substring(0, 20).PadRight(23, '.') : text;
    }
}

<div class="tab-pane-wdgt clearfix">
    <div class="secondary-sub-hdr">
        <h4>GPS Data</h4>
    </div>
    <div class="tab-pane-body clearfix">
        <div class="tab-pane-section col-md-4">
            <div class="form-group">
                <label class="col-md-6">Medicare ID</label>
                @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.HICN, new { @tabindex = "-1", @readonly = "readonly", @class = "form-control" })
            </div>
            <div class="form-group">
                <label class="col-md-6">Member ID</label>
                @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.MemberId, new { @tabindex = "-1", @readonly = "readonly", @class = "form-control" })
            </div>
            <div class="form-group">
                <label class="col-md-6">GPS Household ID</label>
                @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.HouseholdId, "{0:MM/dd/yyyy}", new { @tabindex = "-1", @readonly = "readonly", @class = "form-control" })
            </div>
            <div class="form-group">
                <label class="col-md-6">Contract Number</label>
                @Html.TextBoxFor(xx => xx.objDOGEN_GPSData.ContractNumber, new { @tabindex = "-1", @readonly = "readonly", @class = "form-control" })
            </div>
            <div class="form-group">
